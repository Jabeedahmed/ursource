{
  "name": "vscode-tfs",
  "main": "./lib/vscode-tfs",
  "version": "0.4.0",
  "description": "TFS Visual Studio Code integration.",
  "author": {
    "name": "Ivan Gabriele",
    "email": "ivan.gabriele@gmail.com",
    "url": "http://www.ivangabriele.com"
  },
  "license": "MIT",
  "keywords": [
    "tfs",
    "team",
    "foundation",
    "server",
    "microsoft"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ivangabriele/vscode-tfs.git"
  },
  "bugs": {
    "url": "https://github.com/ivangabriele/vscode-tfs/issues"
  },
  "homepage": "https://github.com/ivangabriele/vscode-tfs#readme",
  "readmeFilename": "README.md",
  "engines": {
    "vscode": "^0.10.1"
  },
  "dependencies": {
    "tfs": "1.3.*"
  },
  "devDependencies": {
    "grunt": "^0.4.5",
    "grunt-contrib-jshint": "^0.12.0",
    "grunt-githooks": "^0.5.0",
    "grunt-jsdoc": "^1.1.0",
    "grunt-mocha-test": "^0.12.7",
    "jshint-stylish": "^2.1.0",
    "mocha": "^2.4.5",
    "vscode": "^0.11.4"
  },
  "script": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "commands": [
      {
        "command": "vscode-tfs.add",
        "title": "TFS: Add"
      },
      {
        "command": "vscode-tfs.checkin",
        "title": "TFS: Check In"
      },
      {
        "command": "vscode-tfs.checkout",
        "title": "TFS: Check Out for Edit"
      },
      {
        "command": "vscode-tfs.get",
        "title": "TFS: Get Latest Version"
      },
      {
        "command": "vscode-tfs.list",
        "title": "TFS: List Commands"
      },
      {
        "command": "vscode-tfs.status",
        "title": "TFS: Status (Pending Changes)"
      },
      {
        "command": "vscode-tfs.undo",
        "title": "TFS: Undo Pending Changes"
      }
    ],
    "keybindings": [
      {
        "command": "vscode-tfs.list",
        "key": "Alt+T"
      }
    ]
  },
  "displayName": "TFS",
  "publisher": "ivangabriele",
  "icon": "res/icon.png",
  "galleryBanner": {
    "color": "#912a2a",
    "theme": "dark"
  },
  "categories": [
    "Other"
  ]
}
